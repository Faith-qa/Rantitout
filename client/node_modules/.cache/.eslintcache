nst cdate = new Date(chatdate)\n  \n  const userid = userp._id\n\n  const url = `${process.env.REACT_APP_BASE_URL}/api/v1/chats/${userid}/${cdate}`;\n\n\n\n  \n  const handleKey = e=>{\n    e.code===\"Enter\" && handlesearch(chatdate);\n  }\n\n  // const handleSelect =async ()=>{\n  //    console.log(chat.messages)\n\n  // }\n\n  return (\n    <div className='search'>\n      <div className=\"searchForm\">\n        <input type=\"date\" onKeyDown={handleKey} onChange={e=>setChatdate(e.target.value)} value={chatdate}/>\n      </div>\n      {err && <span>chat not found</span>}\n      {chat && <div className=\"userChat\">\n        <img src=\"https://images.pexels.com/photos/1967902/pexels-photo-1967902.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2\" alt=\"\"/>\n        <div className=\"userChatInfo\" onClick={console.log(chat.messages)}></div>\n        <span>{chatdate}</span>\n      </div>}\n    </div>\n  )\n}\n","/home/faith/personal_project/TalktoMe/client/src/components/Chats.jsx",["134","135","136","137","138"],[],"/home/faith/personal_project/TalktoMe/client/src/components/Messages.jsx",["139","140"],[],"/home/faith/personal_project/TalktoMe/client/src/components/Message.jsx",["141","142","143"],[],"import React from 'react'\nimport Add from '../img/avator.jpeg'\nimport { useChatContext} from \"../hooks/useChatContext\";\n\nexport const Message = ({message}) => {\n  const {_messages, chat_date} = useChatContext()\n\n  //console.log(\"these are :\", message)\n  return (\n    <div className='message owner'>\n        <div className=\"messageInfo\">\n            <img src=\"https://images.pexels.com/photos/1967902/pexels-photo-1967902.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2\" alt=\"\"/>\n            <span>just now</span>\n        </div>\n        <div className='messageContent'>\n            <p>{message.chat1}</p>\n            <img src=\"https://images.pexels.com/photos/1967902/pexels-photo-1967902.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2\" alt=\"\"